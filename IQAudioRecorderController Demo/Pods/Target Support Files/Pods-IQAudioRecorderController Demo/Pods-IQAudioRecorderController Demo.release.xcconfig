GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FDWaveformView" "${PODS_ROOT}/Headers/Public/SCSiriWaveformView"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/FDWaveformView" "$PODS_CONFIGURATION_BUILD_DIR/SCSiriWaveformView"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/FDWaveformView" -isystem "${PODS_ROOT}/Headers/Public/SCSiriWaveformView"
OTHER_LDFLAGS = $(inherited) -ObjC -l"FDWaveformView" -l"SCSiriWaveformView" -framework "AVFoundation" -framework "CoreGraphics" -framework "Foundation" -framework "MediaPlayer" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
